<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="sales" extends="smartdealer-package" namespace="/">
		<action name="salesorder" class="com.adibrata.smartdealer.action.sales.SalesOrderAction" method="execute">
			<result name="start">/pages/sales/order/paging.jsp</result>
			<result name="search">/pages/sales/order/paging.jsp</result>
			<result name="entry">/pages/sales/order/entry.jsp</result>
			<result name="add">/pages/sales/order/entry.jsp</result>
			<result name="del">/pages/sales/order/entry.jsp</result>
			<result name="save">/pages/sales/order/paging.jsp</result>
			<result name="back">/pages/sales/order/paging.jsp</result>
		</action>
		<action name="salesretur" class="com.adibrata.smartdealer.action.sales.SalesReturAction" method="execute">
			<result name="start">/pages/sales/retur/paging.jsp</result>
			<result name="search">/pages/sales/retur/paging.jsp</result>
			<result name="entry">/pages/sales/retur/entry.jsp</result>
			<result name="add">/pages/sales/retur/entry.jsp</result>
			<result name="del">/pages/sales/retur/entry.jsp</result>
			<result name="save">/pages/sales/retur/paging.jsp</result>
			<result name="back">/pages/sales/retur/paging.jsp</result>
		</action>
		<action name="salesinvoice" class="com.adibrata.smartdealer.action.sales.SalesInvoiceAction" method="execute">
			<result name="start">/pages/sales/retur/paging.jsp</result>
			<result name="search">/pages/sales/retur/paging.jsp</result>
			<result name="entry">/pages/sales/retur/entry.jsp</result>
			<result name="add">/pages/sales/retur/entry.jsp</result>
			<result name="del">/pages/sales/retur/entry.jsp</result>
			<result name="save">/pages/sales/retur/paging.jsp</result>
			<result name="back">/pages/sales/retur/paging.jsp</result>
		</action>
	</package>
</struts>
