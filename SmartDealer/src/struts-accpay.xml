<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="accpay" extends="smartdealer-package" namespace="/">
		<action name="accpay"
			class="com.adibrata.smartdealer.action.accpay.AccPayAction" method="execute">
			<result name="start">/Pages/AccPay/Selection/ApSelection.jsp</result>
			<result name="search">/Pages/AccPay/Selection/ApSelection.jsp</result>
			<result name="back">/Pages/AccPay/Selection/ApSelection.jsp</result>
			<result name="select">/Pages/AccPay/Selection/ApSelectionEntry.jsp
			</result>
			<result name="success">/Pages/AccPay/Selection/ApSelection.jsp</result>
			<result name="error">/Pages/AccPay/Selection/ApSelectionEntry.jsp
			</result>
			<result name="save">/Pages/AccPay/Selection/ApSelectionEntry.jsp
			</result>
		</action>
		<action name="pvdisburse"
			class="com.adibrata.smartdealer.action.accpay.PaymentVoucherAction"
			method="execute">
			<result name="start">/pages/setting/assetmaster/pageAssetMaster.jsp
			</result>
			<result name="search">/pages/setting/assetmaster/pageAssetMaster.jsp
			</result>
			<result name="edit">/pages/setting/assetmaster/editAssetMaster.jsp
			</result>
			<result name="del">/pages/setting/assetmaster/pageAssetMaster.jsp
			</result>
			<result name="save">/pages/setting/assetmaster/pageAssetMaster.jsp
			</result>
			<result name="back">/pages/setting/assetmaster/pageAssetMaster.jsp
			</result>
			<result name="add">/pages/setting/assetmaster/addAssetMaster.jsp
			</result>
			<result name="save">/pages/setting/assetmaster/pageAssetMaster.jsp
			</result>
		</action>

	</package>
</struts>