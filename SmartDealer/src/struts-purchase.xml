<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="purchase" extends="smartdealer-package" namespace="/">
		<action name="purchaseorder" class="com.adibrata.smartdealer.action.purchase.PurchaseOrderAction" method="execute">
			<result name="start">/Pages/Purchase/Order/paging.jsp</result>
			<result name="search">/Pages/Purchase/Order/paging.jsp</result>
			<result name="entry">/Pages/Purchase/Order/entry.jsp</result>
			<result name="add">/Pages/Purchase/Order/entry.jsp</result>
			<result name="del">/Pages/Purchase/Order/entry.jsp</result>
			<result name="save">/Pages/Purchase/Order/paging.jsp</result>
			<result name="back">/Pages/Purchase/Order/paging.jsp</result>
		</action>
		<action name="purchaseinvoice" class="com.adibrata.smartdealer.action.purchase.PurchaseInvoiceAction" method="execute">
			<result name="start">/Pages/Purchase/Receive/paging.jsp</result>
			<result name="search">/Pages/Purchase/Receive/paging.jsp</result>
			<result name="entry">/Pages/Purchase/Receive/entry.jsp</result>
			<result name="save">/Pages/Purchase/Receive/paging.jsp</result>
			<result name="back">/Pages/Purchase/Receive/paging.jsp</result>
		</action>
		<action name="purchaseretur" class="com.adibrata.smartdealer.action.purchase.PurchaseReturAction" method="execute">
			<result name="start">/Pages/Purchase/Retur/paging.jsp</result>
			<result name="search">/Pages/Purchase/Retur/paging.jsp</result>
			<result name="entry">/Pages/Purchase/Retur/entry.jsp</result>
			<result name="add">/Pages/Purchase/Retur/entry.jsp</result>
			<result name="del">/Pages/Purchase/Retur/entry.jsp</result>
			<result name="save">/Pages/Purchase/Retur/paging.jsp</result>
			<result name="back">/Pages/Purchase/Retur/paging.jsp</result>
		</action>
	</package>
</struts>
