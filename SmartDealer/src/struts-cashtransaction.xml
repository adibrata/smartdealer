<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="cashtransaction" extends="smartdealer-package" namespace="/">
		<action name="advancecash" class="com.adibrata.smartdealer.action.cashtransaction.AdvanceClassAction" method="execute">
			<result name="start">/pages/setting/assetdoc/pageAssetDoc.jsp</result>
			<result name="search">/pages/setting/assetdoc/pageAssetDoc.jsp</result>
			<result name="edit">/pages/setting/assetdoc/editAssetDoc.jsp</result>
			<result name="del">/pages/setting/assetdoc/pageAssetDoc.jsp</result>
			<result name="save">/pages/setting/assetdoc/pageAssetDoc.jsp</result>
			<result name="back">/pages/setting/assetdoc/pageAssetDoc.jsp</result>
			<result name="add">/pages/setting/assetdoc/addAssetDoc.jsp</result>
			<result name="save">/pages/setting/assetdoc/pageAssetDoc.jsp</result>
		</action>
		<action name="paymentrequest" class="com.adibrata.smartdealer.action.cashtransaction.PaymentRequestAction" method="execute">
			<result name="start">/pages/setting/assetmaster/pageAssetMaster.jsp</result>
			<result name="search">/pages/setting/assetmaster/pageAssetMaster.jsp</result>
			<result name="edit">/pages/setting/assetmaster/editAssetMaster.jsp</result>
			<result name="del">/pages/setting/assetmaster/pageAssetMaster.jsp</result>
			<result name="save">/pages/setting/assetmaster/pageAssetMaster.jsp</result>
			<result name="back">/pages/setting/assetmaster/pageAssetMaster.jsp</result>
			<result name="add">/pages/setting/assetmaster/addAssetMaster.jsp</result>
			<result name="save">/pages/setting/assetmaster/pageAssetMaster.jsp</result>
		</action>
		<action name="pettycash" class="com.adibrata.smartdealer.action.cashtransaction.PettyCashAction" method="execute">
			<result name="start">/pages/setting/bankAccount/pageBankAccount.jsp</result>
			<result name="search">/pages/setting/bankAccount/pageBankAccount.jsp</result>
			<result name="edit">/pages/setting/bankAccount/editBankAccount.jsp</result>
			<result name="del">/pages/setting/bankAccount/pageBankAccount.jsp</result>
			<result name="save">/pages/setting/bankAccount/pageBankAccount.jsp</result>
			<result name="back">/pages/setting/bankAccount/pageBankAccount.jsp</result>
			<result name="add">/pages/setting/bankAccount/addBankAccount.jsp</result>
			<result name="save">/pages/setting/bankAccount/pageBankAccount.jsp</result>
		</action>

	</package>
</struts>